{
  "name": "learn-webdev-tetris",
  "version": "1.1.0",
  "description": "This is my own implementation of a tetris game with the sole purpose to learn new things. Game logic is implemented in a [custom hook](https://www.npmjs.com/package/use-tetris)",
  "main": "index.js",
  "scripts": {
    "start": "webpack serve --open --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js",
    "analyze": "webpack --config webpack.prod.js --analyze",
    "test": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bboydflo/learn-webdev-tetris.git"
  },
  "author": "Florin Cosmin Onciu",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bboydflo/learn-webdev-tetris/issues"
  },
  "homepage": "https://florin-cosmin-tetris.netlify.app/",
  "dependencies": {
    "preact": "10.5.13",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "use-tetris": "0.4.0"
  },
  "devDependencies": {
    "@types/html-webpack-plugin": "3.2.5",
    "@types/node": "15.12.2",
    "@types/raf": "3.4.0",
    "@types/react": "17.0.11",
    "@types/react-dom": "17.0.7",
    "copy-webpack-plugin": "9.0.0",
    "css-loader": "5.2.6",
    "esbuild-loader": "2.13.1",
    "html-webpack-plugin": "5.3.1",
    "mini-css-extract-plugin": "1.6.0",
    "style-loader": "2.0.0",
    "typescript": "4.3.3",
    "webpack": "5.39.1",
    "webpack-bundle-analyzer": "4.4.2",
    "webpack-cli": "4.7.2",
    "webpack-dev-server": "5.1.0",
    "webpack-merge": "5.8.0"
  }
